{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:allow-local-networking",
    "shell:default",
    "shell:allow-open",
    "shell:allow-spawn",
    "shell:allow-execute",
    "shell:allow-kill",
    "shell:allow-stdin-write",
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "log:default"
  ],
  "platforms": ["windows", "macos", "linux"]
}
